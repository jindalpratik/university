   "source": [
    "def factorial(a):\n",
    "    n = 1\n",
    "    for i in range(1 , a+1):\n",
    "        n = n * i\n",
    "    print(n)\n",
    "\n",
    "x = int(input())\n",
    "factorial(x)"
   ]
  }
